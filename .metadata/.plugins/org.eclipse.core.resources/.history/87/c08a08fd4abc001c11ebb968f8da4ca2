package aula20221404;

public class TiposPrimitivos {
	
	//varaiveis de classes numericas recebe valor 0 padrão
	static int numero;
	
	//varaiveis de classes boolean recebe valor false
	static int numero;

	public static void main(String[] args) {
		
		//byte: até 127 / não utilizado mais
		byte vbyte = 1;
		
		//mais usados para inteiros
		int vint =1;
		long vlong = 12312;
		double vdouble = 12.5;
		
		char vchar = 'a';
		boolean vboolean= true;
		
		Wrappers

	}

}
